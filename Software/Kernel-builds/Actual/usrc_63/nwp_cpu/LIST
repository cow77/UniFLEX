      wzenum= 1...8, sn= 0...7                     *  5025 1A   01                 sec              make reg bits + '10'  5027 59                      rolb             sock TX buffer  5028 1C   FE                 clc  502A 59                      rolb                     *  502B 1A   01                 sec  502D 59                      rolb             write bit     '1'                     *  502E 58                      lslb             variable size '00'  502F 58                      lslb             xxx 10 100                     *                     ***        trfr    B,F  5030 39                      rts                                          *                     * translate socket register in rx buffer address (data xfer)                     * prepares bit [7...0]                     *               5031  tsk2rx    equ    *  5031                         trfr   E,B  5031 1F                      fcb    $1fFIO Simulation Structure             09:47:58  Dec 11, 2023   Page   96wzspi, basic spi routines                             5032 E9                      fcb    E<<4|B                               endm  5033 5A                      decb             wzenum= 1...8, sn= 0...7                     *  5034 1A   01                 sec              make reg bits + '11'  5036 59                      rolb             sock RX buffer  5037 1A   01                 sec              make reg bits + '11'  5039 59                      rolb                     *  503A 1C   FE                 clc  503C 59                      rolb             read bit     '0'                     *  503D 58                      lslb             variable size '00'  503E 58                      lslb             xxx 11 000                     ***        trfr    B,F  503F 39                      rts                                          *                     * rdsk2fb, read socket data in fifo buffer                     * Y=sock, U=sock info                     * E=socket#                     *                     * return, D=xferred count                     *  5040 34   76       RDSK2FB   pshs   d,x,y,u  5042 BD   4F70               jsr    GSRRXRS   Received size  5045 10A3 4A                 cmpd   wzrqln,u  has fio max size incorporated  5048 23   02                 bls    01f  504A EC   4A                 ldd    wzrqln,u  mandatory size  504C 1F   01       01        tfr    D,X       size  504E ED   E4                 std    0,s       save xfrerred old  D  5050 ED   48                 std    wzxfer,u  5052 27   3C                 beq    15f       no data                     *  5054 BD   4F87               jsr    GSRRXRP  5057 34   06                 pshs   d         save socket read pointer  5059 1F   02                 tfr    D,Y                     *  505B CE   8080               ldu    #SPIBASE  505E 34   01                 pshs   cc  5060                         pshsw  5060 1038                    fdb    $1038                               endm  5062 34   40                 pshs   u  5064 1A   50                 orcc   #$50      disable interrupts  5066 86   2D                 lda    #SPI_RST+SPI_SR_+SPI_CR_+SPI_AUT set CS low  5068 A7   C4                 sta    spicmd,u  506A 10AF 44                 sty    hibyta,u  start address                      >506D BD   5031               jsr    tsk2rx    SOCK# to buffer address                       5070 E7   44                 stb    hibyta,uFIO Simulation Structure             09:47:58  Dec 11, 2023   Page   97wzspi, basic spi routines                             5072 108E 0020               ldy    #fifo  5076 E7   44                 stb    hibyta,u  shift out first data byte  5078 33   45                 leau   lobyta,u  lobyte is the first byte shifted in  507A                         trfr   X,W  507A 1F                      fcb    $1f  507B 16                      fcb    X<<4|W                         