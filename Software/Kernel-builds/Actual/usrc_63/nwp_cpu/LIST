            socket register  5039 1A   01                 sec  503B 59                      rolb             xxx 01 ...                     *  503C                         trfr   B,F  503C 1F                      fcb    $1f  503D 9F                      fcb    B<<4|F                               endm  503E 39                      rts                                          *                     * translate socket register in final address (data xfer)                     * prepares bit [7...0]                     *               503F  tsk2tx    equ    *  503F                         trfr   E,B  503F 1F                      fcb    $1f  5040 E9                      fcb    E<<4|B                               endm  5041 5A                      decb             wzenum= 1...8, sn= 0...7                     *  5042 1A   01                 sec              make reg bits + '10'  5044 59                      rolb             sock TX buffer  5045 1C   FE                 clc  5047 59                      rolb                     *  5048 1A   01                 sec  504A 59                      rolb             write bit     '1'                     *  504B 58                      lslb             variable size '00'  504C 58                      lslb             xxx 10 100                     *                     ***        trfr    B,F  504D 39                      rts                                          *                     * translate socket register in rx buffer address (data xfer)                     * prepares bit [7...0]                     *               504E  tsk2rx    equ    *  504E                         trfr   E,B  504E 1F                      fcb    $1fFIO Simulation Structure             21:28:20  Dec 11, 2023   Page   96wzspi, basic spi routines                             504F E9                      fcb    E<<4|B                               endm  5050 5A                      decb             wzenum= 1...8, sn= 0...7                     *  5051 1A   01                 sec              make reg bits + '11'  5053 59                      rolb             sock RX buffer  5054 1A   01                 sec              make reg bits + '11'  5056 59                      rolb                     *  5057 1C   FE                 clc  5059 59                      rolb             read bit     '0'                     *  505A 58                      lslb             variable size '00'  505B 58                      lslb             xxx 11 000                     ***        trfr    B,F  505C 39                      rts                                          *                     * rdsk2fb, read socket data in fifo buffer                     * Y=sock, U=sock info                     * E=socket#                     *                     * return, D=xferred count                     *  505D 34   76       RDSK2FB   pshs   d,x,y,u  505F BD   4F8D               jsr    GSRRXRS   Received size  5062 10A3 4A                 cmpd   wzrqln,u  has fio max size incorporated  5065 23   02                 bls    01f  5067 EC   4A                 ldd    wzrqln,u  mandatory size  5069 1F   01       01        tfr    D,X       size  506B ED   E4                 std    0,s       save xfrerred old  D  506D ED   48                 std    wzxfer,u  506F 27   3C                 beq    15f       no data                     *  5071 BD   4FA4               jsr    GSRRXRP  5074 34   06                 pshs   d         save socket read pointer  5076 1F   02                 tfr    D,Y                     *  5078 CE   8080               ldu    #SPIBASE  507B 34   01                 pshs   cc  507D                         pshsw  507D 1038                    fdb    $1038                               endm  507F 34   40                 pshs   u  5081 1A   50                 orcc   #$50      disable interrupts  5083 86   2D                 lda    #SPI_RST+SPI_SR_+SPI_CR_+SPI_AUT set CS low  5085 A7   C4                 sta    spicmd,u  5087 10AF 44                 sty    hibyta,u  start address                      >508A BD   504E               jsr    tsk2rx    SOCK# to buffer address                       508D E7   44                 stb    hibyta,uFIO Simulation Structure             21:28:20  Dec 11, 2023   Page   97wzspi, basic spi routines                             508F 108E 0020               ldy    #fifo  5093 E7   44                 stb    hibyta,u  shift out first data byte  5095 33   45                 leau   lobyta,u  lobyte is the first byte shifted in  5097                         trfr   X,W  5097 1F                      fcb    $1f  5098 16                      fcb    X<<4|W                               endm  5099                         tfm4   U,Y  5099 113B                    fdb    $113b  509B 32                      fcb    U<<4|Y                               endm  509C 35   40                 puls   u  509E 86   65                 lda    #SPI_CS_+SPI_RST+SPI_SR_+SPI_CR_  50A0 A7   C4                 sta    spicmd,u  50A2                         pulsw  restore   E:F  50A2 1039                    fdb    $1039                               endm  50A4 35   01                 puls   cc                     *  50A6 35   06                 puls   d         old read pointer  50A8 E3   E4                 addd   0,s       adjust transferred  50AA BD   4FB3               jsr    PSRRXRP   update pointer                     *  50AD 35   F6       15        puls   d,x,y,u,pc                                                               *                     * wrfb2sk,write fifo buffer to socket buffer                     * Y=sock                     * E=socket#                     * on return D= xferred count                     *  50AF 34   76       WRFB2SK   pshs   d,x,y,u  50B1 BD   4F41               jsr    GSRTXFR   TX free size  50B4 10A3 4A                 cmpd   wzrql