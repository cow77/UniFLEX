         * hardware register orgaization                     *               8080  SPIBASE   equ    $8080     hardware base address                       0000                         org    0FIO Simulation Structure             21:28:11  Dec 11, 2023   Page   71wzspi, basic spi routines                                                *  0000               spicmd    rmb    1         HW control register               0000  spista    equ    spicmd    HW status register  0001               spicon    rmb    1         [W]   HW aux control register  0002                         rmb    2         dummy not used                     * shift register access  0004               hibyta    rmb    1         [R/W] hibyte auto 8 clock pulses for SR  0005               lobyta    rmb    1         [R/W] lobyte auto 8 clock pulses for SR  0006               hibyts    rmb    1         [R/W] hibyte static (no clock)  0007               lobyts    rmb    1         [R/W] lobyte static (no clock)                     *               0080  QFF1      equ    %10000000 [R]                          X               0040  SPI_CS_   equ    %01000000 [R/W] spi chip CS_           1               0020  SPI_RST   equ    %00100000 [R/W] spi chip RST_          1               0010  SPI_IEN   equ    %00010000 [R/W] spi chip INT enabled   0               0008  SPI_AUT   equ    %00001000 [R/W] auto advance           0               0004  SPI_SR_   equ    %00000100 [R/W] shift register MR_     1               0002  SPI_HLD   equ    %00000010 [R/W] HOLD                   0               0001  SPI_CR_   equ    %00000001 [W]   HC163 MR_              1               0001  SPI_IRQ   equ    %00000001 [R]   /IRQ bit               1                     *               0003  RSVREG    equ    %00000011 reserved register address                       4AC7                         org    oldorg                                          *                     * spinit, init the SPI hardware                     *               4AC7  spinit    equ    *  4AC7 86   65                 lda    #SPI_CS_+SPI_RST+SPI_SR_+SPI_CR_ set idle  4AC9 B7   8080               sta    SPIBASE+spicmd  4ACC 39                      rts                                          *                     * setup inital port for SPI access                     *                     * A,B are lost A=rwflag, F=register, X=offset                     * F=corrected register bits [7...3]                     * A= R/W flag 1=write/ 0= read                     * X,Y untouched                     *               4ACD  spistr2   equ    *  4ACD CE   8080               ldu    #SPIBASE  SPI base TODO  4AD0                         trfr   F,B  4AD0 1F                      fcb    $1f  4AD1 F9                      fcb    F<<4|B                               endm  4AD2 5D                      tstb             register pointer  4AD3 27   09                 beq    01f       OK, valid  4AD5 34   04                 pshs   b  4AD7 C4   03                 andb   #RSVREG   reserved loctation  4AD9 5D                      tstb             xxx10xxx  is invalid!FIO Simulation Structure             21:28:12  Dec 11, 2023   Page   72wzspi, basic spi routines                             4ADA 35   04                 puls   b  4ADC 27   1A                 beq    09f       invalid access                     * adjust READ/WRITE bit  4ADE 4D            01        tsta             cmd flag  4ADF 27   04                 beq    04f  4AE1 1A   01                 sec              write   100  4AE3 20   02                 bra    05f  4AE5 1C   FE       04        clc              read    000                     *  4AE7 59            05        rolb             set READ/WRITE bit  [2...0]  4AE8 58                      lslb             set variable length data [00]  4AE9 58                      lslb                     *                     * here the actual SPI action starts                     *  4AEA                         seti   disable   interrupts  4AEA 1A   50                 orcc   #FF|IF                               endm  4AEC 12                      nop                     *       lda    #SPI_RST+SPI_SR_+SPI_CR_+SPI_AUT+SPI_IEN low  4AED 86   2D                 lda    #SPI_RST+SPI_SR_+SPI_CR_+SPI_AUT low  4AEF A7   C4                 sta    spicmd,u  4AF1 AF   44                 stx    hibyta,u  and shift out  4AF3 E7   44                 stb    hibyta,u  and shift out  4AF5 1A   04                 sez  4AF7 39                      rts  4AF8 1C   FB       09        clz  4AFA 39                      rts                                          *                     * X = offset, unchanged                     * F = register#                     * read BYTE from SPI                     * return B=value, D,X,Y,U preserved                     *               4AFB  byterd    equ    *  4AFB 34   47                 pshs   cc,d,u  4AFD 4F                      clra  4AFE 8D   CD                 bsr    spistr2 >4B00 1026 007E               lbne   09f  4B04 E6   44                 ldb    hibyta,u  extra clocks for first data                     *  4B06 E6   45                 ldb    lobyta,u  read byte  4B08 4F                      clra  4B09 ED   61                 std    1,s  4B0B 20   69                 bra    sprend                                          *                     * X = offset                     * F = register#                     * read WORD from SPI                     * return D=value,  D,X,Y,U preservedFIO Simulation Structure             21:28:12  Dec 11, 2023   Page   73wzspi, basic spi routines                                                *               4B0D  wordrd    equ    *  4B0D 34   47                 pshs   cc,d,u  4B0F 4F                      clra >4B10 BD   4ACD               jsr    spistr2  4B13 26   6D                 bne    09f  4B15 E6   44                 ldb    hibyta,u  extra clocks for first data                     *  4B17 A6   45                 lda    lobyta,u  read byte  4B19 E6   45                 ldb    lobyta,u  read byte  4B1B ED   61                 std    1,s  4B1D 20   57                 bra    sprend                                          *                     * Y=mem pointer, D=count (bytes)                     * F = register#                     * read bytes from SPI into memory location                     * return D,X,Y,U preserved                     *               4B1F  bmread    equ    *  4B1F 34   77                 pshs   cc,d,x,y,u  4B21 4F                      clra  4B22 BD   4FA4               jsr    GSRRXRP  4B25 1F   01                 tfr    d,x  4B27 CE   8080               ldu    #SPIBASE  4B2A 34   40                 pshs   u  4B2C 1A   50                 orcc   #$50  4B2E 86   2D                 lda    #SPI_RST+SPI_SR_+SPI_CR_+SPI_AUT  4B30 A7   C4                 sta    spicmd,u  4B32 AF   44                 stx    hibyta,u  start address  4B34 BD   504E               jsr    tsk2rx  4B37 E7   44                 stb    hibyta,u  4B39 EC   63                 ldd    3,s       old D  4B3B                         trfr   D,W       count  4B3B 1F                      fcb    $1f  4B3C 06                      fcb    D<<4|W                               endm  4B3D 1F   12                 tfr    x,y       mem pointer  4B3F 10AE 67                 ldy    7,s       old Y  4B42 E7   44                 stb    hibyta,u  shift out  4B44 33   45                 leau   lobyta,u  4B46                         tfm4   U,Y  4B46 113B                    fdb    $113b  4B48 32                      fcb    U<<4|Y                               endm  4B49 35   40                 puls   u         old U  4B4B 86   25                 lda    #SPI_RST+SPI_SR_+SPI_CR_  4B4D A7   C4                 sta    spicmd,u                     * X = old read pointer, 0,s byte wount  4B4F 1F   10                 tfr    x,d  4B51 E3   61                 addd   1,s       move pointer  4B53 BD   4FB3               jsr    PSRRXRP   update pointerFIO Simulation Structure             21:28:12  Dec 11, 2023   Page   74wzspi, basic spi routines                             4B56 35   F7                 puls   cc,d,x,y,u,pc                                          *                     * X = offset                     * F = register#                     * write BYTE to SPI, D,X,Y,U preserved                     *               4B58  bytewr    equ    *  4B58 34   47                 pshs   cc,d,u  4B5A 86   01                 lda    #1  4B5C BD   4ACD               jsr    spistr2  4B5F 26   21                 bne    09f                     *  4B61 A6   62                 lda    2,s  4B63 A7   44                 sta    hibyta,u  4B65 20   0F                 bra    sprend                                          *                     * X = offset                     * F = register#                     * write WORD to SPI, D,X,Y,U preserved                     *               4B67  wordwr    equ    *  4B67 34   47                 pshs   cc,d,u  4B69 86   01                 lda    #1  4B6B BD   4ACD               jsr    spistr2  4B6E 26   12                 bne    09f                     *  4B70 EC   61                 ldd    1,s  4B72 A7   44                 sta    hibyta,u  4B74 E7   44                 stb    hibyta,u                     *                     * common exit from SPI action                     *  4B76 86   65       sprend    lda    #SPI_CS_+SPI_RST+SPI_SR_+SPI_CR_                     *sprend  lda     #SPI_CS_+SPI_RST+SPI_SR_+SPI_CR_+SPI_IEN  4B78 A7   C4                 sta    spicmd,u  4B7A A6   E4                 lda    0,s       sez  4B7C 8A   04                 ora    #4  4B7E A7   E4       02        sta    0,s  4B80 35   C7                 puls   cc,d,u,pc                     *  4B82