rmb    1         [R/W] lobyte static (no clock)                     *               0080  QFF1      equ    %10000000 [R]                          X               0040  SPI_CS_   equ    %01000000 [R/W] spi chip CS_           1               0020  SPI_RST   equ    %00100000 [R/W] spi chip RST_          1               0010  SPI_IEN   equ    %00010000 [R/W] spi chip INT enabled   0               0008  SPI_AUT   equ    %00001000 [R/W] auto advance           0               0004  SPI_SR_   equ    %00000100 [R/W] shift register MR_     1FIO Simulation Structure             09:47:50  Dec 11, 2023   Page   71wzspi, basic spi routines                                          0002  SPI_HLD   equ    %00000010 [R/W] HOLD                   0               0001  SPI_CR_   equ    %00000001 [W]   HC163 MR_              1               0001  SPI_IRQ   equ    %00000001 [R]   /IRQ bit               1                     *               0003  RSVREG    equ    %00000011 reserved register address                       4AA7                         org    oldorg                                          *                     * spinit, init the SPI hardware                     *               4AA7  spinit    equ    *  4AA7 86   65                 lda    #SPI_CS_+SPI_RST+SPI_SR_+SPI_CR_ set idle  4AA9 B7   8080               sta    SPIBASE+spicmd  4AAC 39                      rts                                          *                     * setup inital port for SPI access                     *                     * A,B are lost A=rwflag, F=register, X=offset                     * F=corrected register bits [7...3]                     * A= R/W flag 1=write/ 0= read                     * X,Y untouched                     *               4AAD  spistr2   equ    *  4AAD CE   8080               ldu    #SPIBASE  SPI base TODO  4AB0                         trfr   F,B  4AB0 1F                      fcb    $1f  4AB1 F9                      fcb    F<<4|B                               endm  4AB2 5D                      tstb             register pointer  4AB3 27   09                 beq    01f       OK, valid  4AB5 34   04                 pshs   b  4AB7 C4   03                 andb   #RSVREG   reserved loctation  4AB9 5D                      tstb             xxx10xxx  is invalid!  4ABA 35   04                 puls   b  4ABC 27   1A                 beq    09f       invalid access                     * adjust READ/WRITE bit  4ABE 4D            01        tsta             cmd flag  4ABF 27   04                 beq    04f  4AC1 1A   01                 sec              write   100  4AC3 20   02                 bra    05f  4AC5 1C   FE       04        clc              read    000                     *  4AC7 59            05        rolb             set READ/WRITE bit  [2...0]  4AC8 58                      lslb             set variable length data [00]  4AC9 58                      lslb                     *                     * here the actual SPI action starts                     *  4ACA                         seti   disable   interrupts  4ACA 1A   50                 orcc   #FF|IFFIO Simulation Structure             09:47:51  Dec 11, 2023   Page   72wzspi, basic spi routines                                                          endm  4ACC 12                      nop                     *       lda    #SPI_RST+SPI_SR_+SPI_CR_+SPI_AUT+SPI_IEN low  4ACD 86   2D                 lda    #SPI_RST+SPI_SR_+SPI_CR_+SPI_AUT low  4ACF A7   C4                 sta    spicmd,u  4AD1 AF   44                 stx    hibyta,u  and shift out  4AD3 E7   44                 stb    hibyta,u  and shift out  4AD5 1A   04                 sez  4AD7 39                      rts  4AD8 1C   FB       09        clz  4ADA 39                      rts                                          *                     * X = offset, unchanged                     * F = register#                     * read BYTE from SPI                     * return B=value, D,X,Y,U preserved                     *               4ADB  byterd    equ    *  4ADB 34   47                 pshs   cc,d,u  4ADD 4F                      clra  4ADE 8D   CD                 bsr    spistr2  4AE0 1026 0081               lbne   09f  4AE4 E6   44                 ldb    hibyta,u  extra clocks for first data                     *  4AE6 E6   45                 ldb    lobyta,u  read byte  4AE8 4F                      clra  4AE9 ED   61                 std    1,s  4AEB 20   6C                 bra    sprend                                          *                     * X = offset                     * F = register#                     * read WORD from SPI                     * return D=value,  D,X,Y,U preserved                     *               4AED  wordrd    equ    *  4AED 34   47                 pshs   cc,d,u  4AEF 4F                      clra >4AF0 BD   4AAD               jsr    spistr2  4AF3 26   70                 bne    09f  4AF5 E6   44                 ldb    hibyta,u  extra clocks for first data                     *  4AF7 A6   45                 lda    lobyta,u  read byte  4AF9 E6   45                 ldb    lobyta,u  read byte  4AFB ED   61                 std    1,s  4AFD 20   5A                 bra    sprend                                          *                     * Y=mem pointer, D=count (bytes)                     * F = register#                     * read bytes from SPI into memory locationFIO Simulation Structure             09:47:51  Dec 11, 2023   Page   73wzspi, basic spi routines                                                * return D,X,Y,U preserved                     *               4AFF  bmread    equ    *  4AFF 34   77                 pshs   cc,d,x,y,u  4B01 4F                      clra  4B02 BD   4F87               jsr    GSRRXRP  4B05 1F   01                 tfr    d,x  4B07 34   06                 pshs   d         save pointer  4B0